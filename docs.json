              "cn/protocols",
              "cn/residential",
              {
                "group": "入门",
                "pages": [
                  "index",
                  "quickstart",
                  "development"
                ]
              },
              {
                "group": "自定义",
                "pages": [
                  "essentials/settings",
                  "essentials/navigation"
